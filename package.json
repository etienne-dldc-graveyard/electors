{
  "name": "electors",
  "version": "2.0.0",
  "description": "A selector library using a React Hooks like API",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/electors#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/electors/issues"
  },
  "repository": "git+https://github.com/etienne-dldc/electors.git",
  "license": "ISC",
  "author": "Etienne Dldc",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/electors.esm.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "build": "tsdx build --target web",
    "lint": "tsdx lint",
    "pub": "yarn build && np",
    "start": "tsdx watch",
    "test": "tsdx test --coverage",
    "test:watch": "tsdx test --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.6",
    "husky": "^4.2.5",
    "np": "^6.3.2",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7"
  }
}
